const obs=new IntersectionObserver(e=>e.forEach(x=>{if(x.isIntersecting)x.target.classList.add('vis')}),{threshold:.08});
document.querySelectorAll('.rv').forEach(el=>obs.observe(el));
let ly=0;const nav=document.getElementById('nav');
window.addEventListener('scroll',()=>{const y=window.scrollY;nav.classList.toggle('hide',y>ly&&y>80);ly=y;
